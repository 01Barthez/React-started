{
  "name": "react-project-started",
  "private": "true",
  "version": "0.0.0",
  "description": "Ce dépôt est un template moderne et flexible pour démarrer rapidement vos projets React. Il intègre ShadCN, Aeternity, et FarmUI, trois outils puissants pour créer des interfaces utilisateur élégantes et dynamiques. Ce template est optimisé pour les performances, la modularité et une configuration rapide",
  "repository": {
    "type": "git",
    "url": "https://github.com/01Barthez/React-started"
  },
  "homepage": "https://github.com/01Barthez/React-started#readme",
  "readme": "https://github.com/01Barthez/React-started#readme",
  "bugs": {
    "url": "https://github.com/01Barthez/React-started/issues"
  },
  "author": {
    "name": "Barthez Kenwou",
    "email": "kenwoubarthez@gmail.com",
    "url": "https://github.com/01Barthez"
  },
  "license": "MIT",
  "main": "index.html",
  "keywords": [
    "Barthez Kenwou",
    "React",
    "Shadcn",
    "Aceternity",
    "Typescript",
    "Web Developer",
    "Frontend Developer",
    "Fullstack Developer",
    "TailwindCSS"
  ],
  "type": "module",
  "engines": {
    "node": ">=v20.18.1",
    "yarn": ">=1.22.22",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build",
    "start": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "ci": "yarn lint && yarn test && yarn build",
    "docs": "typedoc",
    "clean": "rimraf dist",
    "reinstall": "yarn clean && rimraf node_modules && yarn cache clean && yarn install",
    "commitlint": "commitlint",
    "prepare": "husky install",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.{ts, js, tsx, jsx}": [
      "eslint --fix --max-warnings=10 --cache --config eslint.config.js",
      "prettier --write --config .prettierrc"
    ],
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "env-var": "^7.5.0",
    "lucide-react": "^0.468.0",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "vite": "^6.0.4",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/expect": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "markdownlint": "^0.37.2",
    "markdownlint-cli": "^0.43.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rollup": "^4.28.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typedoc": "^0.27.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}
